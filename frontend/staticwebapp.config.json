{
  "routes": [
    {
      "route": "/api/*",
      "rewrite": "http://application_tier_instance_public_ip:3200/api/:path*"
    },
    {
      "route": "/*",
      "serve": "/index.html",
      "statusCode": 200
    }
  ],
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html"
    }
  }
}
